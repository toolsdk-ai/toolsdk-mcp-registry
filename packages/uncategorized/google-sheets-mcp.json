{
  "type": "mcp-server",
  "packageName": "google-sheets-mcp",
  "description": "Allow AI systems to read, write, and query spreadsheet data via Google Sheets.",
  "url": "https://github.com/domdomegg/google-sheets-mcp.git",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "GOOGLE_ACCESS_TOKEN": {
      "description": "Google OAuth access token with Sheets scopes.",
      "required": true
    },
    "MCP_TRANSPORT": {
      "description": "Transport type.",
      "required": true
    },
    "GOOGLE_CLIENT_ID": {
      "description": "Google OAuth client ID.",
      "required": true
    },
    "GOOGLE_CLIENT_SECRET": {
      "description": "Google OAuth client secret.",
      "required": true
    }
  }
}
