{
  "type": "mcp-server",
  "packageName": "@marcelo-ochoa/server-postgres",
  "description": "MCP server for interacting with PostgreSQL databases",
  "url": "https://github.com/marcelo-ochoa/servers",
  "runtime": "node",
  "license": "unknown",
